<div class="col-md-10 col-md-offset-1 col-sm-10 col-sm-offset-0 col-xs-10 col-xs-offset-1">
 <ul id="user-nav">
  <% var list = navLinks; for (var i = 0; i < list.length; i++) { %>  
    <% var same = (name == list[i].viewName) ? true : false %>
    <a<% if (same) { %> class="active"<% } %> href="<%= (same) ? "#" : list[i].url %>">
      <li><% if (list[i].icon) { %><i class="<%= list[i].icon %>"></i><% } %><%= list[i].name %></li>
    </a>
  <% } %>
 </ul>
</div>
